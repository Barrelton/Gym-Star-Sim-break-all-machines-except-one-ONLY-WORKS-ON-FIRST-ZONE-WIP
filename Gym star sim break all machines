setclipboard('loadstring(game:HttpGet("https://raw.githubusercontent.com/Barrelton/HWID-Extension-4/refs/heads/main/HWID%20Extension%204%20%7C%20Gym%20Star%20Simulator"))()')
while true do
	wait(1)
	game.StarterGui:SetCore("SendNotification", {
	    Title = "New script copied to your clipboard!";
 	   Text = "The new script is copied to your clipboard.";
  	  Duration = 10;
	})
end
